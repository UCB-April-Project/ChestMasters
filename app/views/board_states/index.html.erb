<p id="notice"><%= notice %></p>

<h1>Board States</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @board_states.each do |board_state| %>
      <tr>
        <td><%= link_to 'Show', board_state %></td>
        <td><%= link_to 'Edit', edit_board_state_path(board_state) %></td>
        <td><%= link_to 'Destroy', board_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Board State', new_board_state_path %>
